/interface ethernet
set [ find default-name=ether1 ] advertise=10M-half,10M-full,100M-half,100M-full,1000M-full speed=1Gbps
set [ find default-name=ether2 ] advertise=10M-half,10M-full,100M-half,100M-full,1000M-full speed=1Gbps
set [ find default-name=ether3 ] advertise=10M-half,10M-full,100M-half,100M-full,1000M-full speed=1Gbps
/ip address
add address=210.34.4.2/30 interface=ether1 network=210.34.4.0
add address=210.34.4.5/30 interface=ether2 network=210.34.4.4
add address=210.34.3.1/24 interface=ether3 network=210.34.3.0
/ip dhcp-client
add disabled=no interface=ether1
/ip dhcp-relay
add dhcp-server=210.34.2.2 interface=ether3 name=relay1
add dhcp-server=210.34.2.3 interface=ether3 name=relay2
/ip dns
set servers=8.8.8.8
/ipv6 address
add address=2002:cafe:fe0:ea::2 interface=ether1
add address=2002:cafe:fe0:eb::5 interface=ether2
add address=2002:cafe:fe0:2::1 interface=ether3
/ipv6 dhcp-relay
add dhcp-server=2002:cafe:fe0::2 interface=ether3 name=dhcpServer
/routing ospf interface
add interface=ether3 passive=yes
/routing ospf network
add area=backbone network=210.34.4.0/30
add area=backbone network=210.34.4.4/30
add area=backbone network=210.34.3.0/24
/routing ospf-v3 interface
add area=backbone
add area=backbone interface=ether3 passive=yes
/system ntp client
set primary-ntp=216.239.35.0 secondary-ntp=216.239.35.4
